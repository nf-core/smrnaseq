
nextflow_pipeline {

    name "Test Workflow main.nf - test_umi"
    script "main.nf"
    profile "test_umi,ci"
    tag "umi"
    tag "pipeline"

    def test_scenario = [
        [
            name: "-profile test_umi"
        ]
    ]

    // Generate tests for each scenario
    test_scenario.each { scenario ->
        test(scenario.name, UTILS.get_test(scenario))
    }

}
