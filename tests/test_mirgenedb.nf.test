
nextflow_pipeline {

    name "Test Workflow main.nf - test_mirgenedb"
    script "main.nf"
    profile "test_mirgenedb,ci"
    tag "mirgenedb"
    tag "pipeline"

    def test_scenario = [
        [
            name: "-profile test_mirgened"
        ]
    ]

    // Generate tests for each scenario
    test_scenario.each { scenario ->
        test(scenario.name, UTILS.get_test(scenario))
    }

}
